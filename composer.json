{
    "name": "axguowen/think-crontab",
    "description": "A Simple Crontab Extend For ThinkPHP",
    "keywords": [
        "crontab",
        "extend",
        "thinkphp"
    ],      
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "workerman/workerman": "^4.1",
        "topthink/framework": "^6.1 || ^8.0",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "think\\crontab\\": "src"
        }
    },
    "extra": {
        "think":{
            "services":[
                "think\\crontab\\Service"
            ],
            "config": {
                "crontab": "src/config.php"
            }
        }
    }
}
